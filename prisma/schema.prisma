generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  //url = "mysql://root:password@127.0.0.1:3306/ichoosesv"
  url      = env("DATABASE_URL")
}

model Submission {
  id              String  @id
  name            String
  text            String
  isHadir         Boolean
  jumlahKehadiran Int
}